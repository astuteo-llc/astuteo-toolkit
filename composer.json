{
    "name": "astuteo/astuteo-toolkit",
    "description": "Various tools that we use across client sites. Only useful for Astuteo projects",
    "type": "craft-plugin",
    "prefer-stable": true,
    "version": "6.0.2",
    "keywords": [],
    "scripts": {
        "changelog": "conventional-changelog",
        "release": "conventional-changelog --commit",
        "release:patch": "conventional-changelog --patch --commit",
        "release:minor": "conventional-changelog --minor --commit",
        "release:major": "conventional-changelog --major --commit"
    },
    "support": {
        "docs": "https://github.com/astuteo-llc/astuteo-toolkit/blob/master/README.md",
        "issues": "https://github.com/astuteo-llc/astuteo-toolkit/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Astuteo",
            "homepage": "https://astuteo.com"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "craftcms/cms": "^5.0.0",
        "ext-json": "*"
    },
    "require-dev": {
        "marcocesarato/php-conventional-changelog": "^1.16"
    },
    "autoload": {
        "psr-4": {
            "astuteo\\astuteotoolkit\\": "src/"
        }
    },
    "extra": {
        "name": "Astuteo Toolkit",
        "handle": "astuteo-toolkit",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/astuteo-llc/astuteo-toolkit/master/CHANGELOG.md",
        "components": {
            "astuteoToolkitService": "astuteo\\astuteotoolkit\\services\\AstuteoToolkitService"
        },
        "class": "astuteo\\astuteotoolkit\\AstuteoToolkit"
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}